{
  "expo": {
    "name": "BanHang",
    "slug": "BanHang",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoDCAT_Trang.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoDCAT_Trang.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoDCAT_Trang.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "BLUETOOTH",
        "BLUETOOTH_AMIN"
      ],
      "package": "com.thanh9462.BanHang"
    },
    "web": {
      "favicon": "./assets/logoDCAT_Trang.jpg"
    }
  }
}
